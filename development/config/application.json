{
  "logger": {
    "level": "debug",
    "development": true,
    "disableCaller": false,
    "encoding": "json",
    "outputPaths": [
      "stdout",
      "/var/lib/atehere/logs/log.txt"
    ],
    "errorOutputPaths": [
      "stderr",
      "stdout"
    ],
    "encoderConfig": {
      "messageKey": "message",
      "levelKey": "level",
      "levelEncoder": "lowercase"
    }
  },
  "api": {
    "version": "v1",
    "name": "atehere",
    "host": "0.0.0.0",
    "port": "8080"
  },
  "db": {
    "driver": "postgres",
    "dsn": "postgres://developer:postgres@db:5432/atehere?sslmode=disable",
    "migrations": "file:///var/lib/atehere/migrations",
    "timeout": "2s",
    "tryCount": 5
  }
}